package collection;

import java.util.ArrayList;

public class C001_ArrayListDemo {
	public static void main(String[] args) {
		
		ArrayList<String> al = new ArrayList<>();
		al.add("java");
		al.add("php");
		al.add("pythone");
		al.add("android");
		
		
		
		for(String s : al)
		{
			System.out.println(s);
		}
		
		
		
		
		
		
		
	}
}
