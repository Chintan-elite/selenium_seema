package collection;

import java.util.ArrayList;

public class C002_ArraylistDemo {
	public static void main(String[] args) {
		
		ArrayList<String> s = new ArrayList<>();
		s.add("cricket");
		s.add("Hockey");
		s.add("Football");
		
		s.add(1,"Vollyball");
		
		for(String str : s)
		{
			System.out.println(str);
		}
		
		
	}
}
