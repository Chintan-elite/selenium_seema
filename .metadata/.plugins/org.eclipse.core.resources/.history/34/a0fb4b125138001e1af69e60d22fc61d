package oops;

public class Product {
		
		private String name;
		private int price;
		private int qty;
		
		public void setdata(String name, int price, int qty)
		{
			this.name = name;
			this.price = price;
			this.qty = qty;
		}
		
		
		public void display()
		{
			System.out.println(name+" "+price+" "+qty);
		}
		
}
