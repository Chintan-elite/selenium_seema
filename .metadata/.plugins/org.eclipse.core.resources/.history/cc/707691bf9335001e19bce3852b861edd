package oops;


class Calc
{
	public void print()
	{
		System.out.println("running print...");
	}
	
	public void add(int a, int b)
	{
		int c = a+b;
		System.out.println("Addition is : "+c);
	}
	
}

public class O005_MethodManipulation {
	public static void main(String[] args) {
		
		
		Calc c = new Calc();
		c.print();
		
		
	}
}
