package oops;


class A1
{
	public void add(int a, int b)
	{
		int c  =a+b;
		System.out.println("Additoin is : "+c);
	}
	
	public void add(int a, int b, int c)
	{
		int c  =a+b+c;
		System.out.println("Additoin is : "+c);
	}
}
public class O009_verloading {
	public static void main(String[] args) {
		
		
		A1 a = new A1();
		a.add(10, 20);
		a.add(10, 20,30);
		
		
		
	}
}
