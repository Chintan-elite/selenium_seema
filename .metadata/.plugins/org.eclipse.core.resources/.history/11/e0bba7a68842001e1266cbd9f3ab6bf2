package fileio;

import java.io.File;

public class F005_file {
	public static void main(String[] args) {
		
		
		File file = new File("D://test//abc.txt");
		
		if(file.exists())
		{
			System.out.println("File available");
		}
		else
		{
			System.out.println("File not exist !!!");
		}
		
		
		
		
	}
}
