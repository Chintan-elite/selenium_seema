package oops;

class A
{
	int id = 10;
	
	public void display()
	{
		System.out.println("Running display method");
	}
}


class B
{
	public void sample()
	{
		System.out.println("Running sample method...");
	}
}

public class O004_InheritanceDemo {
	public static void main(String[] args) {
		
		B b = new B();
		b.sample();
	
		
		A a = new A();
		a.display();
		
	}
}
