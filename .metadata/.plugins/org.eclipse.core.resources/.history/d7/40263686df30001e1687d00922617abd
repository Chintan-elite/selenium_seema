package oops;

class Emp
{
	int id;
	String name;
	String email;
	
	Emp(int a, String b, String c)
	{
		id = a;
		name = b;
		email = c;
	}
	
	public void display()
	{
		System.out.println(id+" "+name+" "+email);
	}
}

public class O003_ConstructorImpl {
	public static void main(String[] args) {
		
		Emp e = new Emp(001,"Parth","parth@gmail.com");
		e.display();
		
		
	}
}
